{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testEnvironment": "jsdom",
  "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "coverageDirectory": "coverage",
  "coverageProvider": "v8",
  "coverageThreshold": {
    "global": {
      "functions": 100
    }
  }
}